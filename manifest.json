{
  "manifest_version": 2,
  "name": "Clipboard data generator",
  "version": "0.0.2",
  "description": "A handy data generator that fills the clipboard",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["storage"]
}
