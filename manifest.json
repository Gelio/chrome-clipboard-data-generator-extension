{
  "manifest_version": 2,
  "name": "Random data generator",
  "version": "1.0",
  "description": "A handy random data generator",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["storage"]
}
